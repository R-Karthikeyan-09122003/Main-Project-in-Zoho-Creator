			{
				type =  approval
				form = Leave_Request
				display fields = [Apply_Date,Employee_ID_Employee_Name,Role,Leave_Start_Date,Leave_End_Date,No_of_Days_Leave,Leave_Status]
				on start
				{
				}
				on level 1
				{
					on approve 
					{
						actions 
						{
							sendmail
							(
								from : zoho.loginuserid
								to : approver.submitter
								subject : "Approval Request"
								message : "Your request has been approved."
							)
						}
						actions 
						{
						on load
						(
							input.Leave_Status = "Approvel";
												new = Trip_Schedules_Ex_sub[ID != null];
												startdate = input.Leave_Start_Date;
												enddate = input.Leave_End_Date;
												emtydate = startdate;
												no_of_days = startdate.daysBetween(enddate);
												emty = " ".leftpad(no_of_days).toList("");
												for each  d in emty
												{
													// 	info emtydate;
													for each  k in new
													{
														if(k.Trip_Date == emtydate && k.Driver_ID == input.Employee_ID_Employee_Name.Staff_ID && k.Trip_Status == "Scheduled")
														{
															k.Driver_ID=null;
															k.Driver_Name=null;
														}
														else if(k.Trip_Date == emtydate && k.Conductor_ID == input.Employee_ID_Employee_Name.Staff_ID && k.Trip_Status == "Scheduled")
														{
															k.Conductor_ID=null;
															k.Conductor_Name=null;
														}
														else if(k.Trip_Date == emtydate && k.Cleaner_ID == input.Employee_ID_Employee_Name.Staff_ID && k.Trip_Status == "Scheduled")
														{
															k.Cleaner_Name=null;
															k.Cleaner_ID=null;
														}
													}
													emtydate = emtydate.addDay(1);
												}
						)
						}
					}
					on reject
					{

						actions 
						{
							sendmail
							(
								from : zoho.loginuserid
								to : approver.submitter
								subject : "Approval Request"
								message : "Your request has been rejected."
							)
						}
						actions 
						{
						on load
						(
							input.Leave_Status = "Reject";
						)
						}
					}
				}
			}
