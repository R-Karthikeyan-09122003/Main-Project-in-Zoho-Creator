			Cancel_Schedules as "Cancel Schedules"
			{
				type =  functions
				form = Trip_Schedules_Ex_sub
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						input.Trip_Status = "Cancelled";
										var = Employee_Details[Staff_ID == input.Driver_ID || Staff_ID == input.Conductor_ID || Staff_ID == input.Cleaner_ID].Email;
										sendmail
										[
											from :zoho.adminuserid
											to :var
											subject :"Your trip is cancel"
