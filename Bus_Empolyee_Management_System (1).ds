				record event = on add or edit

				field rules
				{
					actions 
					{
						hide Bus_ID,Bus_Status,Count_For_Schedules;
					}
				}

			}
			Set_Field_Rules as "Set Field Rules"
			{
				type =  form
				form = Employee_Details
				record event = on add or edit

				field rules
				{
					actions 
					{
						hide Staff_ID,Status,Status1,Present_Status,Check_In_OR_Check_Out_Count;
					}
					actions 
					{
						disable Salary,Joing_Date;
					}
					actions 
					{
					hide delete row of Uplode_Documents;
					}
					actions 
					{
					hide add row of Uplode_Documents;
					}
					actions  (Role == 313844000000140043)
					{
						show Driving_Licence_Number,Uplode_Driving_Licence;
					}
				}

			}
			Salary_Data_Fatch as "Salary Data Fatch"
			{
				type =  form
				form = Employee_Details
				record event = on add or edit

				on user input of Role
				{
					actions 
					{
						custom deluge script
						(
											if(Role != null)
										{
											new = Role_Based_Salary_Details[ID == input.Role];
											input.Salary = new.Salary;
										}
										else
										{
											input.Salary = null;
										}
						)
					}
				}

			}
			DOB_Validation as "DOB Validation"
			{
				type =  form
				form = Employee_Details
				record event = on add or edit

				on user input of Date_of_Birth
				{
					actions 
					{
						custom deluge script
						(
											if(input.Date_of_Birth != null)
										{
											new = input.Date_of_Birth.yearsBetween(zoho.currentdate);
											if(new < 18)
											{
												alert "You are not Eligible & You are Age is = " + new;
												input.Date_of_Birth = null;
											}
										}
						)
					}
				}

			}
			Driver_Details_Validation as "Driver Details Validation"
			{
				type =  form
				form = Employee_Details
				record event = on add or edit

				on validate
				{
					actions 
					{
						custom deluge script
						(
											if(input.Uplode_Driving_Licence.isEmpty() && input.Role == 313844000000140043)
										{
											alert "Plz Uplode Driving Licence";
											cancel submit;
										}
										if(input.Uplode_Documents.Criminal_Record_Police_Verification == null || input.Uplode_Documents.Medical_Fitness_Certificate == null)
										{
											alert "Uplode Required Documents";
											cancel submit;
										}
										if(input.Phone_Number1.length() > 13 || input.Phone_Number1.length() < 13)
										{
											alert "Enter Valid Number";
											cancel submit;
										}
						)
					}
				}

