					actions 
					{
					on click
					(
						openUrl("#Form:Active_Staff_Allote?Bus_Number=" + input.Bus_Number + "&Date_field=" + input.Trip_Date,"popup window");
										new = Trip_Schedules_Ex_sub[ID != null];
										for each  rec in new
										{
											if(rec.Drive_Role == null && rec.Drive_Role != input.Drive_Role)
											{
												info "varuthu";
											}
										}
					)
					}
				}
			}
